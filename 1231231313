    end)
    local FirstToggle = MainPage.AddToggle("AntiFlash", nil, function(Value) 
        local Loop
        local loopFunction = function()
            local WS = game.Players.LocalPlayer.PlayerGui.MainScreenGui:FindFirstChild('whiteScreen')
            if WS then
                WS:Destroy()
            end
        end;
        local Start = function()
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
        end;
        Start()	
        repeat wait() until Value == false
        Pause()
    end)

    local MainButton = MainPage.AddButton("Dash", function()
        assert(getrawmetatable)
        gmt = getrawmetatable(game)
        setreadonly(gmt, false)
        old = gmt.__namecall
        gmt.__namecall =
            newcclosure(
                function(self, ...)
                local args = {...}
                if tostring(args[1]) == "BreathingHAMON" then
                    return
                elseif tostring(args[1]) == "TeleportDetect" then
                    return
                elseif tostring(args[1]) == "CHECKER_1" then
                    return
                elseif tostring(args[1]) == "CHECKER" then
                    return
                elseif tostring(args[1]) == "GUI_CHECK" then
                    return
                elseif tostring(args[1]) == "OneMoreTime" then
                    return
                elseif tostring(args[1]) == "checkingSPEED" then
                    return
                elseif tostring(args[1]) == "BANREMOTE" then
                    return
                elseif tostring(args[1]) == "PERMAIDBAN" then
                    return
                elseif tostring(args[1]) == "KICKREMOTE" then
                    return
                elseif tostring(args[1]) == "BR_KICKPC" then
                    return
                end
                return old(self, ...)
                end)
        wait(1)
        
        setfpscap(9999)
        
        function Ringtone(ID)
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
            local Tool = nil
            if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
            Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
            game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
            end
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
            game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
            game:GetService("Players").LocalPlayer.Character
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Fire Event
            local args = {
                [1] = "RingTone",
                [2] = ID
                
            }
            game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
            game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
            game:GetService("Players").LocalPlayer.Backpack
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
            if Tool ~= true then
            if Tool then
            Tool.Parent = game:GetService("Players").LocalPlayer.Character
            end
            end
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Play Sound 
            game.Players:chat("/e cm")
            end
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Stop Audio
            function Stop()
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
            local Tool = nil
            if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
            Tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
            game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Parent = game:GetService("Players").LocalPlayer.Backpack
            end
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Phone
            game:GetService("Players").LocalPlayer.Backpack["[Phone]"].Parent =
            game:GetService("Players").LocalPlayer.Character
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Unequip Phone
            game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent =
            game:GetService("Players").LocalPlayer.Backpack
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Equip Saved Tool
            if Tool ~= true then
            if Tool then
            Tool.Parent = game:GetService("Players").LocalPlayer.Character
            end
            end
            end
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Change Id
            function Change(ID)
            --//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\\-- Save Tool
